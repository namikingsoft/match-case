{
  "name": "match-case",
  "version": "0.0.5",
  "description": "Functional pattern matcher",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dtsm": "dtsm",
    "clean": "rm -rf lib",
    "compile": "tsc -p . --outDir lib",
    "build": "npm run clean && npm run compile",
    "test": "NODE_PATH=src mocha test/**/*.ts",
    "test:watch": "npm test -- --watch --reporter min",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/namikingsoft/match-case.git"
  },
  "keywords": [
    "match",
    "matcher",
    "case",
    "when",
    "then",
    "functional",
    "immutable"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "immutable": "^3.7.5"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "dtsm": "^0.12.0",
    "espower-typescript": "^3.0.0",
    "mocha": "^2.3.3",
    "power-assert": "^1.1.0",
    "typescript": "^1.7.0-dev.20151006"
  }
}
