{
  "name": "match-case",
  "version": "0.0.7",
  "description": "Functional pattern matcher",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "tsc -p . --outDir lib",
    "build": "npm run clean && npm run compile",
    "test": "test/all.sh",
    "test:lite": "NODE_PATH=src mocha test/**/*.ts",
    "test:watch": "npm run test:lite -- --watch --reporter min",
    "postinstall": "if [ \"$NODE_ENV\" != production ]; then dtsm install; fi",
    "prepublish": "if [ \"$NODE_ENV\" != test ]; then npm test; fi"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/namikingsoft/match-case.git"
  },
  "keywords": [
    "match",
    "matcher",
    "case",
    "when",
    "then",
    "functional",
    "immutable"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "immutable": "^3.7.5"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "coveralls": "^2.11.4",
    "dtsm": "^0.12.0",
    "espower-typescript": "^3.0.0",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "power-assert": "^1.1.0",
    "remap-istanbul": "^0.4.0",
    "typescript": "^1.7.0-dev.20151006"
  }
}
